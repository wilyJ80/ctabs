digraph TSV_Lexer {
	rankdir=LR;
	node [shape = circle, fontname="Arial"];

	// States
	Start [shape = doublecircle, label="START"];
	Collecting [label="COLLECTING"];
	EndField [shape = doublecircle, label="ENDFIELD"];
	IsField [label="ISFIELD"]
	Field [shape = doublecircle, label="FIELD"];

	Start -> Start [label="nonprinting"]
	Start -> Collecting [label="printing"]
	Collecting -> Collecting [label="printing"]
	Collecting -> EndField [label="\\n, \\r"]
	Collecting -> IsField [label="\\t"]
	IsField -> EndField [label="\\n, \\r"]
	IsField -> Field [label="other*"]

	label = "\nTSV Lexer\nNo escaping support";
}
